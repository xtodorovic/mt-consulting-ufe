{
  "name": "mt-consulting-ufe",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/mt-consulting-ufe/mt-consulting-ufe.esm.js",
  "exports": {
    ".": {
      "import": "./dist/mt-consulting-ufe/mt-consulting-ufe.esm.js",
      "require": "./dist/mt-consulting-ufe/mt-consulting-ufe.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "convert-openapi": "js-yaml  ./api/consulting.openapi.yaml > .openapi.json",
    "mock-api": "open-api-mocker --schema .openapi.json --port 8080",
    "start:app": "stencil build --dev --watch --serve",
    "start:mock": "run-s convert-openapi mock-api",
    "start": "run-p -r start:mock start:app",
    "openapi": "openapi-generator-cli generate",
    "build": "stencil build --docs",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.20.0",
    "@stencil/core": "^4.31.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "open-api-mocker": "^2.0.0",
    "puppeteer": "^24.3.0"
  },
  "license": "MIT",
  "dependencies": {
    "@material/web": "^2.3.0",
    "@stencil/router": "^1.0.1"
  }
}
